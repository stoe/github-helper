{
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": [
    "github"
  ],
  "extends": [
    "plugin:github/recommended"
  ],
  "globals": {
    "chrome": "readonly",
    "document": "readonly",
    "MutationObserver": "writable"
  },
  "rules": {
    "camelcase": [
      "error",
      {
        "allow": [
          "per_page"
        ]
      }
    ],
    "import/no-commonjs": "off"
  }
}